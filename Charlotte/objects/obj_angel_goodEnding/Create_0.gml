dialogueSequence[0] = ["Nice to see you again, Eve, or I guess Charlotte now. I'm here to bring you back to the afterlife."]
dialogueSequence[1] = ["You see, this was a test. Nobody simply 'escapes' from the afterlife. Your soul has to be permitted to leave."]
dialogueSequence[3] = ["We wanted to see if humans are capable of change, to see if our decision to send humans to a certain afterlife for all eternity might be flawed."]
dialogueSequence[7] = ["Your actions here on Earth have shown us that we still have much work to do. I'm here to bring you to your new afterlife in Heaven for the good you've done."]